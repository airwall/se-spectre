section#customers
  .heading-wrap
    h3 custommers
  .main-content
    .table
      .th-row.hidden
        .th.identificator Identifier
        .th.date
        .th.compact
      - @customers.each do |c|
        .th-row
          .td.identificator
            = link_to c['identifier'], customer_path(id: c['id'])
          .td.date
            = render 'shared/connection', id: c['id']
